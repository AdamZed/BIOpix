package movement;

/**
 * @version 1.0.1
 */

public class Pair
{
	private int X = 0;
	private int Y = 0;
	
	public Pair() {}

	public Pair(int x, int y)
	{
		this.X = x;
		this.Y = y;
	}
	
	public int getX () { return X; }
	public int getY () { return Y; }	
	public void setX (int newX) { X = newX; }
	public void setY (int newY) { Y = newY; }	
	
}
